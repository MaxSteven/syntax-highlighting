<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base 16 Duotone-DarkEarth">

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#fcb07d" background="#322d29"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#ac9b91"/>
  <style name="CurrentLine" background="#85786f"/>
  <style name="LineNumber" foreground="#85786f"/>
  <style name="CurrentLineNumber" foreground="#ac9b91" bold="true"/>
  <style name="SearchResult" background="4c4642"/>
  <style name="Parentheses" background="#695f59" />

  <!-- ??? -->
  <style name="SearchScope" foreground="#000000" background="#ffffff"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#85786f"/>
  <style name="Occurrences.Rename" background="#ac9b91"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#bfa05a"/>

  <!-- language primitives -->
  <style name="Number" foreground="#bfa05a"/>
  <style name="String" foreground="#a89771"/>
  <style name="Keyword" foreground="#c8ab98"/>
  <style name="Operator" foreground="#fcb07d"/>
  <style name="Preprocessor" foreground="#d7b975"/>
  <style name="Label" foreground="#c08e6d"/>

  <!-- types, classes -->
  <style name="Type" foreground="#ccad66"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#d7b975"/>
  <style name="VirtualMethod" foreground="#d7b975" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#fed4b8"/>
  <style name="Local" foreground="#fcb07d"/>
  <style name="Static" foreground="#fcb07d"/>
  <style name="JsScopeVar" foreground="#fcb07d" italic="true"/>
  <style name="JsImportVar" foreground="#fcb07d" italic="true"/>
  <style name="JsGlobalVar" foreground="#fcb07d" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#695f59"/>

  <!-- comments -->
  <style name="Comment" foreground="#85786f"/>
  <style name="DisabledCode" foreground="#85786f"/>
  <style name="Doxygen.Comment" foreground="#85786f"/>
  <style name="Doxygen.Tag" foreground="#85786f"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#c8ab98"/>
  <style name="Binding" foreground="#c08e6d"/>
  <style name="QmlExternalId" foreground="#d7b975" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#d7b975" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#c8ab98"/>
  <style name="DiffLocation" foreground="#d7b975"/>
  <style name="AddedLine" foreground="#a89771"/>
  <style name="RemovedLine" foreground="#ccad66"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#c8ab98"/>
  <style name="DiffContextLine" foreground="#d7b975"/>
  <style name="DiffSourceLine" foreground="#a89771"/>
  <style name="DiffSourceChar" foreground="#ccad66" background="#4c4642"/>
  <style name="DiffDestLine" foreground="#a89771"/>
  <style name="DiffDestChar" foreground="#ccad66" background="#4c4642"/>
</style-scheme>
