[global_config]
  title_transmit_bg_color = "#705ec9"
  title_inactive_bg_color = "#77738c"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#2a2734:#705ec9:#968f88:#d48535:#de9145:#988dce:#e8a35e:#a391fd:#77738c:#705ec9:#968f88:#d48535:#de9145:#988dce:#e8a35e:#eeebff"
    foreground_color = "#a391fd"
    background_color = "#2a2734"
    cursor_color = "#a391fd"

  [[Base16 Duotone-Dark Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#2a2734:#705ec9:#968f88:#d48535:#de9145:#988dce:#e8a35e:#a391fd:#77738c:#705ec9:#968f88:#d48535:#de9145:#988dce:#e8a35e:#eeebff"
    foreground_color = "#a391fd"
    background_color = "#2a2734"
    cursor_color = "#a391fd"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Duotone-Dark Dark
    [[[window0]]]
      type = Window
      parent = ""
