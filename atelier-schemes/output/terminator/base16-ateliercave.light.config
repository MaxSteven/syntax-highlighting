[global_config]
  title_transmit_bg_color = "#cf598a"
  title_inactive_bg_color = "#695e78"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#1e1a23:#cf598a:#74875a:#a08f3b:#6577d2:#955ae7:#328fae:#8f859d:#695e78:#cf598a:#74875a:#a08f3b:#6577d2:#955ae7:#328fae:#f4f0fa"
    foreground_color = "#5c526b"
    background_color = "#f4f0fa"
    cursor_color = "#5c526b"

  [[Base16 Atelier Cave Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#f4f0fa:#cf598a:#74875a:#a08f3b:#6577d2:#955ae7:#328fae:#8f859d:#695e78:#cf598a:#74875a:#a08f3b:#6577d2:#955ae7:#328fae:#1e1a23"
    foreground_color = "#5c526b"
    background_color = "#f4f0fa"
    cursor_color = "#5c526b"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Atelier Cave Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
