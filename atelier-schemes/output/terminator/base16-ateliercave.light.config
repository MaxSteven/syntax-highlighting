  [[Base16 Atelier Cave Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#f4f0fa:#cf598a:#74875a:#a08f3b:#6577d2:#955ae7:#328fae:#9b8eaf:#6e588d:#cf598a:#74875a:#a08f3b:#6577d2:#955ae7:#328fae:#302046"
    foreground_color = "#624e7e"
    background_color = "#f4f0fa"
    cursor_color = "#624e7e"

[layouts]
