[global_config]
  title_transmit_bg_color = "#cf598a"
  title_inactive_bg_color = "#6e588d"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#302046:#cf598a:#74875a:#a08f3b:#6577d2:#955ae7:#328fae:#9b8eaf:#6e588d:#cf598a:#74875a:#a08f3b:#6577d2:#955ae7:#328fae:#f4f0fa"
    foreground_color = "#624e7e"
    background_color = "#f4f0fa"
