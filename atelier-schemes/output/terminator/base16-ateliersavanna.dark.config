    copy_on_selection = True
    allow_bold = False
    palette = "#1a231d:#b16139:#489963:#a07e3b:#478c90:#55859b:#1c9aa0:#809988:#5a7262:#b16139:#489963:#a07e3b:#478c90:#55859b:#1c9aa0:#e8f7ed"
