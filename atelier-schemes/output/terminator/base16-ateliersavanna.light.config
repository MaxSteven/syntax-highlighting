[global_config]
  title_transmit_bg_color = "#b16139"
  title_inactive_bg_color = "#4f725b"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#15281c:#b16139:#489963:#a07e3b:#478c90:#55859b:#1c9aa0:#739c80:#4f725b:#b16139:#489963:#a07e3b:#478c90:#55859b:#1c9aa0:#ddeee3"
    foreground_color = "#41624c"
    background_color = "#ddeee3"
    cursor_color = "#41624c"

  [[Base16 Atelier Savanna Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#ddeee3:#b16139:#489963:#a07e3b:#478c90:#55859b:#1c9aa0:#739c80:#4f725b:#b16139:#489963:#a07e3b:#478c90:#55859b:#1c9aa0:#15281c"
    foreground_color = "#41624c"
    background_color = "#ddeee3"
    cursor_color = "#41624c"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Atelier Savanna Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
